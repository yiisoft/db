{
    "name": "yiisoft/db",
    "type": "library",
    "description": "Yii DataBase Library",
    "keywords": [
        "yii",
        "database"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/db",
        "issues": "https://github.com/yiisoft/db/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "psr/log": "^1.1",
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/data": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/profiler": "^3.0@dev",
        "yiisoft/strings": "^3.0@dev"
    },
    "require-dev": {
        "yiisoft/composer-config-plugin": "^1.0@dev",
        "phpunit/phpunit": "^9.0",
        "yiisoft/aliases": "^1.0",
        "yiisoft/db-mysql": "^3.0@dev",
        "yiisoft/db-pgsql": "^3.0@dev",
        "yiisoft/db-sqlite": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/json": "^3.0@dev",
        "yiisoft/log-target-file": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Db\\": "src",
            "Yiisoft\\Db\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "tests": "config/tests.php"
        }
    },
    "config": {
        "sort-packages": true
    }
}
